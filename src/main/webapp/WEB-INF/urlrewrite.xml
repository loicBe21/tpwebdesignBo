<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>

    <rule>
        <from>/allContents-(\d)+-inteligenceArtifial_actu.html$</from>
        <to>/home?num=$1</to>
    </rule>
    <rule>
        <from>/articles/view-(.*)-(.*)_pages_(.*)_cat_(.*)_cat_n_(.*)$</from>
        <to>/acticle/$2?num=$3&amp;cat_id=$4&amp;cat_name=$5</to>
    </rule>
    <rule>
        <from>/articles/view-(.*)-(.*)_pages_(.*)_h=true$</from>
        <to>/acticle/$2?num=$3</to>
    </rule>
    <rule>
       <from>/allContents-categories_(.*)-([0-9]*)_pages_([0-9]*)_actu.html$</from>
        <to>/byCategorie?cat_id=$2&amp;num=$3&amp;cat_name=$1</to>
    </rule>

</urlrewrite>